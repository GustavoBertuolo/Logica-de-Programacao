#include <stdio.h>
 
int main() {
 
    int a, b, tempo;
    scanf("%d%d", &a, &b);
    if(a>b)
    {
      tempo=24-(a-b);
    }
    else
    {
        if(b>a)
        {
            tempo=b-a;
        }
        else
        {
            if(a==b)
            {
                tempo=24;
            }
        }
    }
    printf("O JOGO DUROU %d HORA(S)\n", tempo);
 
    return 0;
}
