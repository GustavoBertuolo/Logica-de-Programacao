#include <stdio.h>
int main()
{
    int a, b=0;
    scanf("%d", &a);
    if(a%2!=0)
    {
        printf("%d\n", a);
        b++;
    }
    for(b; b<6; b++)
    {
        if(a%2!=0)
        {
            a+=2;
            printf("%d\n", a);
        }
        else if(a%2 == 0)
        {
            a++;
            printf("%d\n", a);
        }
    }
    return 0;
}
