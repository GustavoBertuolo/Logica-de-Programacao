#include <stdio.h>
 
int main() {
 
    int a, b, c, d, e, f, k;
    scanf("%d%d%d%d%d%d", &a, &b, &c, &d, &e, &f);
    k=0;
    if(a>0)
    {
        k++;
    }
    if(b>0)
    {
        k++;
    }
    if(c>0)
    {
        k++;
    }
    if(d>0)
    {
        k++;
    }
    if(e>0)
    {
        k++;
    }
    if(f>0)
    {
        k++;
    }
    printf("%d valores positivos\n", k);
 
    return 0;
}
