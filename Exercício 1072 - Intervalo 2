#include <stdio.h>
int main()
{
    int a, x, m=0, n=0;
    scanf("%d", &a);

    for(a; a>0; a--)
    {
        scanf("%d", &x);
        if(x>=10&&x<=20)
        {
            m++;
        }
        else
        {
            n++;
        }
        
    }
    printf("%d in\n%d out\n", m, n);
    return 0;
}
